#! /bin/sh
# Tmux starter script.

tmux new-session -d -s main
tmux new-window -c ~
tmux new-window -c ~
tmux new-window -c ~
tmux new-window -c ~
tmux new-window -c ~
tmux new-window -c ~/buffered
tmux new-window -c ~/buffered
tmux new-window -c ~
tmux new-window -c ~
tmux select-window -t 1
tmux -2 attach-session -t main
